<div class="artist-detail">

    <div class="artist-detail__cover" style="background-image: url('{{gruppo.coverImage}}')">
        <div class="content-wrapper">
            <div class="visual__text">
                <h1>{{gruppo.name}}</h1>
                <p>{{gruppo.informations}}</p>
            </div>
        </div>
    </div>
    <div class="disco-info">
        <div class="content-wrapper disco-info__wrapper">
            <div class="disco-text-ctr">
                <div class="disco-text__wrapper">
                    <h2 class="disco-info__title">INTRODUCING</h2>
                    <p class="disco-info__text">
                        {{gruppo.biography}}</p>
                    <div class="more-info">
                        <p><strong>genere:</strong> {{gruppo.genere}}</p>
                        <p><strong>location:</strong> {{gruppo.location}}</p>
                        <p><a href="mailto:{{gruppo.email}}"><strong>email</strong></a></p>
                        <p><a href="{{gruppo.website}}"><strong>site</strong></a></p>
                    </div>


                </div>
                <h2 class="disco-info__title">MEMBRI DEL GRUPPO</h2>
                <ul>
                    {{#each gruppo.membri}}
                        <li class="membro__thumb">

                            <div class="text-ctr">
                                <a href="/group-member/{{this._id}}"><p class="membro__thumb-name">{{this.name}}</p></a>
                                <ul class="strumenti">
                                    {{#each this.strumenti}}
                                        <li class="strumento">
                                            <p>{{this}}</p>
                                        </li>
                                    {{/each}}
                                </ul>
                            </div>


                        </li>
                    {{/each}}

                </ul>
                <div>


                    <iframe width="100%" height="360px"
                            src="https://www.youtube.com/embed/eyrAK_OHN90?rel=0&amp;showinfo=0" frameborder="0"
                            allowfullscreen></iframe>
                </div>
            </div>
            <div class="disco__right">
                <figure class="disco-cover">
                    <img src="{{gruppo.profileImage}}">
                </figure>
                <div>
                    <ul>
                        <li>
                            <iframe style="border: 0; width: 100%; height: 42px;"
                                    src="https://bandcamp.com/EmbeddedPlayer/album=745234224/size=small/bgcol=ffffff/linkcol=0687f5/track=2656877289/transparent=true/"
